{
    "name": "felipedamacenoteodoro/laravel-whatsapp-notification-channel",
    "description": "Whatsapp Notifications Channel for Laravel",
    "keywords": [
        "whatsapp notification",
        "laravel",
        "whatsapp",
        "notification",
        "whatsapp notifications channel"
    ],
    "homepage": "https://github.com/felipedamacenoteodoro/laravel-whatsapp-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Damaceno Teodoro",
            "email": "felipe.devops@gmail.com",
            "homepage": "https://www.linkedin.com/in/felipedamacenoteodoro/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/contracts": "^11.3.1",
        "illuminate/notifications": "^11.35.1",
        "illuminate/support": "^11.35.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3 || ^1.4",
        "phpunit/phpunit": "^7.0 || ^8.5.21 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Whatsapp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Whatsapp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Whatsapp\\WhatsappServiceProvider"
            ]
        }
    }
}
